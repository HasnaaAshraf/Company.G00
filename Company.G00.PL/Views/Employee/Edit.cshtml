@model CreateEmployeeDto;

@{
    ViewData["Title"] = "Edit";
}

<br/>

<h1>Edit Employee</h1>

<br/>

<form asp-action="Edit" enctype="multipart/form-data">

    <partial name="EmployeePartialView/_CreateAndEditPartialView" />

    <div>
        <input type="submit" value="Save" class="btn btn-outline-primary mt-3" />
        <a asp-action="Index" class="mt-3 ms-2 btn btn-light">Back To List</a>
    </div>

</form>


@section ValidationSection
{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}

