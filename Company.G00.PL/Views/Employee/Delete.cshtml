@model Employee

@{
    ViewData["Title"] = "Delete";
}

<br/>

<h1>Delete Page</h1>

<br/>

<form asp-action="Delete" enctype="multipart/form-data">

    <partial name="EmployeePartialView/_DetailsDeletePartialView" />
    <input asp-for="ImageName" hidden/>   

    <div>
        <input type="submit" value="Delete" class="mt-3 btn btn-outline-danger"/>
